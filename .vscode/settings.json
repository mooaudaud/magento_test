{
  "emeraldwalk.runonsave": {
    "commands": [
      {
        "match": ".*",
        "isAsync": true,
        "cmd": "docker exec magento-begin mkdir -p $(sed 's/${fileBasename}//g' <<<'/${relativeFile}')"
      },
      {
        "match": ".*",
        "isAsync": true,
        "cmd": "docker cp ${file} magento-begin:/${relativeFile}"
      }
    ]
  }
}